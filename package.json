{
  "name": "trustroots",
  "description": "Trustroots.org",
  "version": "0.3.2",
  "author": "https://www.trustroots.org/foundation",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Trustroots/trustroots"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 9"
  ],
  "scripts": {
    "prestart": "if [ ! -d node_modules ]; then npm install --quiet; fi",
    "start": "concurrently --raw --kill-others --kill-others-on-fail 'gulp' 'npm run start:worker' 'maildev' 'npm run agendash'",
    "start:docker": "npm run prestart && concurrently --raw --kill-others --kill-others-on-fail 'gulp --skipBower' 'npm run start:worker'",
    "start:skipBower": "concurrently --raw --kill-others --kill-others-on-fail 'gulp --skipBower' 'npm run start:worker' 'maildev' 'npm run agendash'",
    "start:prod": "gulp prod",
    "start:worker": "gulp worker:dev",
    "start:worker:prod": "gulp worker:prod",
    "build": "gulp build:dev",
    "build:dev": "gulp build:dev",
    "build:prod": "gulp build:prod",
    "fontello": "gulp fontello",
    "dropdb": "gulp dropdb",
    "lint": "gulp lint",
    "test": "gulp test",
    "test:client": "gulp test:client",
    "test:client:watch": "gulp test:client:watch",
    "test:server": "gulp test:server",
    "test:server:watch": "gulp test:server:watch",
    "migrate": "migrate up",
    "migrate-down": "migrate down",
    "update": "npm install --quiet && npm prune && migrate",
    "postinstall": "bower install --allow-root && bower prune --allow-root",
    "agendash": "./scripts/agendash-dev"
  },
  "dependencies": {
    "acl": "~0.4.10",
    "agenda": "~0.10.0",
    "async": "~2.5.0",
    "autolinker": "~1.4.3",
    "autoprefixer": "~7.1.2",
    "basic-auth": "~1.1.0",
    "body-parser": "~1.17.2",
    "bower": "~1.8",
    "bson": "~1.0.4",
    "chalk": "~2.0.1",
    "compression": "~1.7.0",
    "connect-flash": "~0.1.1",
    "connect-mongo": "~1.3.2",
    "consolidate": "~0.14.1",
    "cookie-parser": "~1.4.3",
    "del": "~3.0.0",
    "eslint-plugin-angular": "~3.0.0",
    "expect-ct": "~0.1.0",
    "exponent-server-sdk": "^2.3.1",
    "express": "~4.15.3",
    "express-paginate": "~0.3.0",
