{
  "name": "trustroots",
  "description": "Trustroots.org",
  "version": "0.3.3",
  "author": "https://www.trustroots.org/foundation",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Trustroots/trustroots"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "browserslist": [
    "last 2 versions",
    "Safari >= 10",
    "iOS >= 10",
    "not ie <= 10",
    "> 1%"
  ],
  "scripts": {
    "prestart": "npm run install-if-no-modules && check-node-version --package",
    "install-if-no-modules": "if [ ! -d node_modules ]; then npm install --quiet; fi",
    "start": "concurrently --raw --kill-others --kill-others-on-fail 'gulp' 'npm run start:worker' 'maildev' 'npm run agendash'",
    "start:docker": "npm run prestart && concurrently --raw --kill-others --kill-others-on-fail 'gulp --skipBower' 'npm run start:worker'",
    "start:skipBower": "concurrently --raw --kill-others --kill-others-on-fail 'gulp --skipBower' 'npm run start:worker' 'maildev' 'npm run agendash'",
    "start:prod": "gulp prod",
    "start:worker": "gulp worker:dev",
    "start:worker:prod": "gulp worker:prod",
    "build": "gulp build:dev",
    "build:dev": "gulp build:dev",
    "build:prod": "gulp build:prod",
    "fontello": "gulp fontello",
    "dropdb": "gulp dropdb",
    "lint": "gulp lint",
    "test": "gulp test",
    "test:client": "gulp test:client",
    "test:client:watch": "gulp test:client:watch",
    "test:server": "gulp test:server",
    "test:server:watch": "gulp test:server:watch",
    "migrate": "migrate up",
    "migrate-down": "migrate down",
    "update": "npm install --quiet && npm prune && migrate",
    "postinstall": "bower install --allow-root && bower prune --allow-root",
    "agendash": "./scripts/agendash-dev",
    "agenda-maintenance": "node ./scripts/db-maintenance/archive-done-agenda-jobs.js"
  },
  "dependencies": {
    "acl": "~0.4.11",
    "agenda": "~1.0.3",
    "async": "~2.6.0",
    "autolinker": "~1.6.2",
    "autoprefixer": "~8.2.0",
    "basic-auth": "~2.0.0",
    "body-parser": "~1.18.2",
    "bower": "~1.8.4",
    "bson": "~1.0.6",
    "chalk": "~2.3.2",
    "check-node-version": "~3.2.0",
    "compression": "~1.7.2",
    "connect-flash": "~0.1.1",
    "connect-mongo": "~1.3.2",
    "cookie-parser": "~1.4.3",
    "del": "~3.0.0",
    "eslint-plugin-angular": "~3.2.0",
    "expect-ct": "~0.1.0",
    "expo-server-sdk": "~2.3.3",
    "express": "~4.16.3",
    "express-paginate": "~0.3.0",
    "express-session": "~1.15.6",
    "fbgraph": "~1.4.4",
    "firebase-admin": "~4.2.0",
    "git-rev": "~0.2.1",
    "glob": "~7.1.1",
    "gm": "~1.23.1",
    "gulp": "~3.9",
    "gulp-angular-templatecache": "~2.2.0",
    "gulp-autoprefixer": "~4.1.0",
    "gulp-bower": "0.0.14",
    "gulp-concat": "~2.6.0",
    "gulp-csso": "~3.0.1",
    "gulp-eslint": "~4.0.2",
    "gulp-fontello": "~0.4.5",
    "gulp-htmlmin": "~4.0.0",
    "gulp-less": "~3.4.0",
    "gulp-load-plugins": "~1.5.0",
    "gulp-ng-annotate": "~2.1.0",
    "gulp-nodemon": "~2.2.1",
    "gulp-print": "~2.0.1",
    "gulp-refresh": "~1.1.0",
    "gulp-rename": "~1.2.2",
    "gulp-shell": "~0.6.5",
    "gulp-sourcemaps": "~2.6.4",
    "gulp-uglify": "~3.0.0",
    "he": "~1.1.1",
    "helmet": "~3.10.0",
    "html-to-text": "~4.0.0",
    "influx": "~5.0.7",
    "juice": "~4.2.3",
    "lodash": "~4.17.5",
    "merge-stream": "~1.0.1",
    "method-override": "~2.3.10",
    "migrate": "~0.2.4",
    "mkdir-recursive": "~0.4.0",
    "mmmagic": "~0.5.0",
    "moment": "~2.22.0",
    "mongodb": "~2.2.35",
    "mongoose": "~4.9.10",
    "mongoose-beautiful-unique-validation": "~5.1.1",
    "mongoose-integer": "~0.1.1",
    "mongoose-paginate": "5.0.3",
