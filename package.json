    "moment": "~2.18.1",
    "mongodb": "~2.2.31",
    "mongoose": "~4.9.10",
    "mongoose-beautiful-unique-validation": "~5.1.1",
    "mongoose-integer": "~0.1.1",
    "mongoose-paginate": "5.0.3",
