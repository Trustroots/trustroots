    "serve-favicon": "~2.5.0",
    "speakingurl": "~14.0.1",
    "stathat": "0.0.8",
    "uuid": "~3.2.1",
    "validator": "~9.4.1",
    "winston": "~2.4.1",
    "winston-papertrail": "~1.0.4",
    "yargs": "~11.0.0"
  },
  "devDependencies": {
    "agendash": "~0.4.0",
    "concurrently": "~3.5.1",
    "faker": "~3.1.0",
    "gulp-mocha": "~3.0.1",
    "jasmine-core": "~2.6.4",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-jasmine": "~1.1.1",
    "karma-mocha-reporter": "~2.2.5",
    "karma-ng-html2js-preprocessor": "~1.0.0",
    "karma-phantomjs-launcher": "~1.0.4",
    "maildev": "~1.0.0-rc3",
    "nodemailer-stub-transport": "~1.1.0",
    "promise": "~8.0.0",
    "proxyquire": "~2.0.1",
    "semver": "~5.5.0",
    "should": "~11.2.1",
    "sinon": "~2.3.7",
    "supertest": "~3.0.0"
  },
  "bugs": {
