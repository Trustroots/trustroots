    "gulp-autoprefixer": "~4.0.0",
    "gulp-bower": "~0.0.13",
    "gulp-concat": "~2.6.0",
    "gulp-csso": "~3.0.0",
    "gulp-eslint": "~2.0.0",
    "gulp-fontello": "~0.4.5",
