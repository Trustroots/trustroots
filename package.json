    "yargs": "~8.0.1"
  },
  "devDependencies": {
    "agendash": "~0.4.0",
    "concurrently": "~3.5.0",
    "faker": "~3.1.0",
    "gulp-mocha": "~3.0.1",
    "jasmine-core": "~2.6.4",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-mocha-reporter": "~2.2.3",
    "karma-ng-html2js-preprocessor": "~1.0.0",
    "karma-phantomjs-launcher": "~1.0.4",
    "maildev": "~1.0.0-rc3",
    "nodemailer-stub-transport": "~1.1.0",
